[program:buzzer_sub]
command={{ webapps_dir }}/buzzer_sub.py -e {{ aws_iot_endpoint }} -r {{ creds_location }}/rootCA.pem -c {{ creds_location }}/certificate.pem -k {{ creds_location }}/private.pem -t {{ flash_mqtt_topic }} -x "\$aws/things/{{ inventory_hostname }}/shadow/update" -p {{ buzzer_pin }}
directory={{ webapps_dir }}
autostart=true
autorestart=true
stdout_logfile={{ log_file }}
stderr_logfile={{ log_file }}
redirect_stderr=true
exitcodes=0
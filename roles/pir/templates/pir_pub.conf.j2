[program:pir_pub-{{ item.value.pin }}]
command={{ webapps_dir }}/pir_pub.py
    -e {{ aws_iot_endpoint }}
    -r {{ creds_location }}/rootCA.pem
    -c {{ creds_location }}/certificate.pem
    -k {{ creds_location }}/private.pem
    -t {{ item.value.topic }} $aws/things/{{ inventory_hostname }}/shadow/update
    -p {{ item.value.pin }}
    -s {{ item.value.name }}
directory={{ webapps_dir }}
autostart=true
autorestart=true
stdout_logfile={{ log_file }}
stderr_logfile={{ log_file }}
redirect_stderr=true
exitcodes=0